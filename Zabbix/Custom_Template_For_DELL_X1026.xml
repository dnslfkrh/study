<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>6.4</version>
    <templates>
        <template>
            <uuid>c0f3a8714d6b4c8596e8b77cc31f1e01</uuid>
            <template>DELL_X1026_CUSTOM_TEMPLATE</template>
            <name>DELL_X1026_CUSTOM_TEMPLATE</name>
            <description>SNMP template with basic system uptime and interface monitoring, filtered for 26 GigabitEthernet ports.</description>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <items>
                <item>
                    <uuid>3d4f061e067549dbb93334aa48fa6401</uuid>
                    <name>System Uptime</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.2.1.1.3.0</snmp_oid>
                    <key>system.uptime</key>
                    <delay>60s</delay>
                    <history>30d</history>
                    <trends>365d</trends>
                    <value_type>UNSIGNED</value_type>
                    <units>uptime</units>
                    <description>System uptime in TimeTicks</description>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <parameters>
                                <parameter>0.01</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <uuid>e20c37b427c748a79f413af86bb92a71</uuid>
                    <name>Network Interfaces Discovery</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#IFNAME},1.3.6.1.2.1.2.2.1.2,{#IFOPERSTATUS},1.3.6.1.2.1.2.2.1.8,{#IFADMINSTATUS},1.3.6.1.2.1.2.2.1.7,{#IFTYPE},1.3.6.1.2.1.2.2.1.3]</snmp_oid>
                    <key>net.if.discovery</key>
                    <delay>3600s</delay>
                    <lifetime>7d</lifetime>
                    <description>Discovery for network interfaces</description>
                    <filter>
                        <evaltype>AND</evaltype>
                        <conditions>
                            <condition>
                                <macro>{#IFADMINSTATUS}</macro>
                                <value>1</value>
                                <formulaid>A</formulaid>
                            </condition>
                            <condition>
                                <macro>{#IFNAME}</macro>
                                <value>@DELL_X1026_INTERFACES</value>
                                <formulaid>B</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <uuid>7f8dfb7cf6494a3888a696c5f84e2f30</uuid>
                            <name>Interface {#IFNAME}: Bits received</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.2.1.2.2.1.10.{#SNMPINDEX}</snmp_oid>
                            <key>net.if.in[{#IFNAME}]</key>
                            <delay>60s</delay>
                            <history>30d</history>
                            <trends>365d</trends>
                            <value_type>UNSIGNED</value_type>
                            <units>bps</units>
                            <description>Incoming traffic on interface {#IFNAME}</description>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <parameters>
                                        <parameter>8</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <uuid>2e24b31f03a74f2f87c77b8f612f31aa</uuid>
                            <name>Interface {#IFNAME}: Bits sent</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.2.1.2.2.1.16.{#SNMPINDEX}</snmp_oid>
                            <key>net.if.out[{#IFNAME}]</key>
                            <delay>60s</delay>
                            <history>30d</history>
                            <trends>365d</trends>
                            <value_type>UNSIGNED</value_type>
                            <units>bps</units>
                            <description>Outgoing traffic on interface {#IFNAME}</description>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <parameters>
                                        <parameter>8</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <uuid>97b3d91bec8a4f0e81d034e073e568c9</uuid>
                            <name>Interface {#IFNAME}: Operational status</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.2.1.2.2.1.8.{#SNMPINDEX}</snmp_oid>
                            <key>net.if.status[{#IFNAME}]</key>
                            <delay>60s</delay>
                            <history>30d</history>
                            <trends>0</trends>
                            <value_type>UNSIGNED</value_type>
                            <description>Operational status of interface {#IFNAME} (1=up, 2=down)</description>
                        </item_prototype>
                        <item_prototype>
                            <uuid>4a5b6c7d8e9f4a1b9c3d4e5f6a7b8c9d</uuid>
                            <name>Interface {#IFNAME}: Speed</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.2.1.2.2.1.5.{#SNMPINDEX}</snmp_oid>
                            <key>net.if.speed[{#IFNAME}]</key>
                            <delay>300s</delay>
                            <history>30d</history>
                            <trends>365d</trends>
                            <value_type>UNSIGNED</value_type>
                            <units>bps</units>
                            <description>Interface {#IFNAME} speed in bits per second</description>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>